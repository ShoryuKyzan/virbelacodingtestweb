{
  "name": "virbelacodingtestweb",
  "version": "1.0.0",
  "description": "Joseph Fleming's coding test for web, for virbela. Original node/typescript template from: https://github.com/microsoft/TypeScript-Node-Starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShoryuKyzan/virbelacodingtestweb"
  },
  "author": "Joseph Fleming",
  "scripts": {
    "build": "tsc && npm run lint",
    "debug": "npm run build && npm run watch-debug",
    "lint": "tsc --noEmit && eslint \"{test,src}/**/*.{js,ts}\" --quiet --fix",
    "serve": "nodemon src/server.ts",
    "start": "docker-compose build && docker-compose up -d",
    "stop": "docker-compose down",
    "test": "cross-env NODE_ENV=test jest"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-promise-router": "^4.1.1",
    "sequelize": "^6.17.0",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/sequelize": "^4.28.11",
    "@types/supertest": "^2.0.11",
    "@types/test-console": "^2.0.0",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2",
    "test-console": "^2.0.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
